   "source": [
    "# 计算 hmac_sha1 加密值\n",
    "import hmac\n",
    "import hashlib\n",
    "\n",
    "message = 'wxnacy'\n",
    "key = 'wxnacy.com'\n",
    "\n",
    "h = hmac.new(key.encode('utf-8'), message.encode('utf-8'), hashlib.sha1)\n",
    "# 计算 16 进制编码字符串\n",
    "h.hexdigest()"
   ]
  },
  {
   "cell_type": "code",
